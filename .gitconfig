[user]
	name = Kyle Jake Plimack
	email = kyle.plimack@gmail.com
[color]
	branch = auto
	diff = auto
	status = auto
[color "branch"]
	current = yellow reverse
       	local = yellow
       	remote = green
[color "diff"]
	meta = yellow bold
       	frag = magenta bold
       	old = red
       	new = cyan
[color "status"]
	added = yellow
       	changed = green
       	untracked = cyan
[alias]
	tree = log --graph --pretty=oneline --abbrev-commit --decorate --color --all
	local = log --pretty=oneline @{u}..HEAD
[apply]
	whitespace = strip
[pager]
	color = true
[status]
	color = true
